<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <configSections>
    <section name="buildMapperConfigSection" type="BuildClient.Configuration.BuildMapperConfigSection,BuildClient" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="PollPeriod" value="60" />
    <add key="BuildDefinitionNameExclusionPattern" value="" />
    <!-- 1. Please provide the value-->
    <add key="TeamFoundationUrl" value="https://my.visualstudio.com/DefaultCollection" />

    <add key="UseCredentialToAuthenticate" value="true"/>
    <add key ="TFSAccountDomain" value=""/>
    <add key ="TFSAccountUserName" value=""/>
    <add key ="TFSAccountPassword" value=""/>
    <add key="DisablePublishNotification" value="true" />
  </appSettings>

  <buildMapperConfigSection>
    <buildMappers> <!-- 2. Please provide the value here-->
      <buildMapper name="build1"
                   TfsProjectToMonitor="MyProject"
                   TfsBuildToMonitor="MyBuild"
                   NotificationAddress="net.tcp://localhost:9090" />
   </buildMappers>
  </buildMapperConfigSection>

  <system.diagnostics>
    <sources>
      <source name="BuildClient.Client" switchValue="Verbose">
        <listeners>
          <add name="ConsoleTraceListener" type="System.Diagnostics.ConsoleTraceListener"></add>
        </listeners>
      </source>
    </sources>

  </system.diagnostics>

</configuration>